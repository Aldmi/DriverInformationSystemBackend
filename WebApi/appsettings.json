{
  "ConnectionStrings": {
    "Mongodb": "mongodb://root:pass12345@mongodb:27017/DIS_Production?authSource=admin&connectTimeoutMS=3000"
  },

  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "File", "Args": { "path": "Logs/log.txt" } }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "Sample"
    }
  },
  
  "AllowedHosts": "*"
}
